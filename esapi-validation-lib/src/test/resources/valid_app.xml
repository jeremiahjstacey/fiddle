<?xml version="1.0" encoding="UTF-8"?>

<esapi:esapi xmlns:xs="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:esapi="http://org.owasp.esapi">

	<validators>
		<validator>
			<id>NOT_NULL</id>
			<type>org.owasp.esapi.validation.objects.NotNullValidator</type>
		</validator>
		<validator>
			<id>IS_NULL</id>
			<type>org.owasp.esapi.validation.objects.NullValidator</type>
		</validator>
		<validator>
			<id>STR_NOT_EMPTY</id>
			<type>org.owasp.esapi.validation.strings.NotEmptyValidator</type>
		</validator>
		<validator>
			<id>STR_Match_ANY</id>
			<type>org.owasp.esapi.validation.MatchAnyString</type>			
		</validator>

		<validator>
			<id>STR_Match_NONE</id>
			<type>org.owasp.esapi.validation.strings.StringPatternValidator
			</type>
			<args>
				<arg>
					<type>java.lang.String</type>
					<value>*</value>
				</arg>
				<arg>
					<type>java.lang.Boolean</type>
					<value>false</value>
				</arg>
			</args>
		</validator>
	</validators>

	<configuration>
	</configuration>

</esapi:esapi>